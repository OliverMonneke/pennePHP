{
    "name": "codersquad/pennephp",
    "type": "library",
    "description": "Simple PHP libraries for faster development",
    "homepage": "http://github.com/Codersquad/pennePHP",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Monneke",
            "email": "oliver@codersquad.de",
            "homepage": "http://pennephp.codersquad.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Codersquad\\Pennephp\\": [ "src" ],
            "Codersquad\\Pennephp\\Tests\\": [ "tests" ]
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
